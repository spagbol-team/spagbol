{
  "name": "spagbol-web-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.eslintignore",
    "lint:format": "prettier  --log-level warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "preview": "vite preview",
    "docker-build": "docker-compose up --build --no-recreate -d",
    "docker-up": "docker-compose up -d",
    "docker-exec": "docker exec -it vite_docker sh",
    "docker-initiate": "make first",
    "docker-dev": "make start"
  },
  "dependencies": {
    "plotly.js-dist-min": "^2.25.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "contributors": [
    {
      "name": "ardhi.rofi",
      "email": "ardhi.rofi@gmail.com"
    }
  ],
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.15",
    "eslint": ">=5.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.28",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.5"
  },
  "main": "index.js",
